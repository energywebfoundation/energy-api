{
  "name": "@energyweb/energy-api-app",
  "version": "0.3.0",
  "description": "An example hosted API using @energyweb/energy-api",
  "author": "EnergyWeb DevHub GmbH; Piotr Kosinski, piotr.kosinski@energyweb.org",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@energyweb/energy-api-influxdb": "0.8.0",
    "@nestjs/common": "7.6.18",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.18",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/swagger": "5.1.1",
    "class-transformer": "0.3.1",
    "class-validator": "0.13.1",
    "json-to-pretty-yaml": "1.2.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@types/express": "4.17.13",
    "@types/node": "13.1.6",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.25.2",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "ts-node": "9.1.1",
    "typescript": "4.4.4"
  },
  "files": [
    "dist"
  ],
  "private": "true"
}
